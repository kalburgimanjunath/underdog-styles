{
  "name": "underdog-styles",
  "version": "2.2.1",
  "description": "Organization wide styles",
  "main": "server/",
  "scripts": {
    "clean": "rm -rf dist/",
    "browser-sync": "browser-sync start --proxy 'localhost:9008' --files='dist/**/*' --no-notify",
    "build": "npm run clean && npm run build-js && npm run build-sass && npm run favicons && npm run icons && npm run images",
    "build-js": "mkdir -p dist/js/ && browserify --outfile dist/js/app.js js/*.js",
    "build-sass": "node-sass --include-path node_modules/ --output dist/css/ --source-map true --output-style compressed scss/",
    "postbuild-sass": "postcss --use autoprefixer --replace dist/css/**/*.css",
    "develop": "nodemon --exec 'npm run build' --watch scss/ --watch js/ --ext scss & npm run browser-sync",
    "favicons": "cp -R favicons/ dist/",
    "icons": "mkdir -p fonts && mkdir -p dist/fonts && npm run icons-svg && npm run icons-ttf && npm run icons-eot && npm run icons-woff && cp fonts/* dist/fonts/",
    "icons-eot": "ttf2eot fonts/underdogio-icons.ttf fonts/underdogio-icons.eot",
    "icons-svg": "svgicons2svgfont icons/*.svg --fontFamily underdogio-icons --output fonts/underdogio-icons.svg",
    "icons-ttf": "svg2ttf fonts/underdogio-icons.svg fonts/underdogio-icons.ttf",
    "icons-woff": "ttf2woff fonts/underdogio-icons.ttf fonts/underdogio-icons.woff",
    "icons-watch": "nodemon --exec 'icons' --watch icons --ext svg",
    "gemini-gather": "gemini gather test/visual/*.js",
    "gemini-gui": "gemini-gui test/visual/*.js",
    "gemini-test": "gemini test --reporter html --reporter flat test/visual/*.js",
    "heroku-postbuild": "npm run build",
    "images": "cp -R images/ dist/img/",
    "lint": "npm run lint-js && npm run lint-sass",
    "lint-js": "eslint js/",
    "lint-sass": "sass-lint-underdog",
    "test": "npm run lint",
    "start": "bin/underdog-styles",
    "start-develop": "nodemon --exec bin/underdog-styles --watch server/ --watch docs/ --ext js,md,hbs",
    "start-webdriver": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update --standalone"
  },
  "homepage": "https://github.com/underdogio/underdog-styles",
  "private": true,
  "devDependencies": {
    "async": "2.0.1",
    "autoprefixer": "6.4.1",
    "browser-sync": "2.15.0",
    "browserify": "13.1.0",
    "eslint": "3.5.0",
    "eslint-config-underdog": "1.3.0",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "gemini": "4.9.3",
    "gemini-gui": "4.3.3",
    "glob": "7.0.6",
    "meta-marked": "0.4.2",
    "node-sass": "3.9.3",
    "nodemon": "1.10.2",
    "postcss-cli": "2.6.0",
    "sass-lint-underdog": "1.3.0",
    "slug": "0.9.1",
    "svg2ttf": "4.0.2",
    "svgicons2svgfont": "5.0.0",
    "ttf2eot": "2.0.0",
    "ttf2woff": "2.0.1",
    "underscore": "1.8.3",
    "webdriver-manager": "10.2.3"
  },
  "dependencies": {
    "inuit.css": "git://github.com/underdogio/inuit.css#2dcfbd3ad85d90a2f62241e764357449df5ee5a6",
    "swipe-js": "2.2.0"
  }
}
